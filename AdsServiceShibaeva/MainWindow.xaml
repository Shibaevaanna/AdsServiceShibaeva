<Window x:Class="AdsServiceShibaeva.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="Сервис объявлений" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>

        <Button x:Name="backbutton" Content="Назад" Style="{StaticResource button}" 
                Click="backbutton_Click" Width="50" Height="30" 
                HorizontalAlignment="Right" VerticalAlignment="Center" Margin="10"/>

        <Frame x:Name="MainFrame" Grid.Row="1" NavigationUIVisibility="Hidden" 
               HorizontalAlignment="Stretch" VerticalAlignment="Stretch"/>
    </Grid>
</Window>